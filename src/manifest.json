{
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Avro",
    "default_icon": {
      "128": "images/i128.png",
      "32": "images/i32.png",
      "16": "images/i16.png"
    }
  },
  "icons": {
    "128": "images/i128.png",
    "32": "images/i32.png",
    "16": "images/i16.png"
  },
  "background": {
    "service_worker": "background.js"
  },

  "permissions": ["storage"],

  "name": "Avro (অভ্র) for Chrome",
  "short_name": "Avro(অভ্র)",
  "version": "1.2.9",
  "description": "Avro extension for chrome. Write Bangla instantly."
}
